{
    "name": "LandmarkDetect-ML-MSET-Train",
    "container-image": {
        "image": "neubiaswg5/w_landmarkdetect-ml-mset-train",
        "type": "singularity"
    },
    "schema-version": "cytomine-0.1",
    "description": "Training phase of the MSET Landmark Detection Algorithm",
    "command-line": "python run.py CYTOMINE_HOST CYTOMINE_PUBLIC_KEY CYTOMINE_PRIVATE_KEY CYTOMINE_ID_PROJECT CYTOMINE_ID_SOFTWARE CYTOMINE_TRAINING_IMAGES MODEL_NJOBS MODEL_RMAX MODEL_R MODEL_P MODEL_NPRED MODEL_NTREES MODEL_NTIMES MODEL_ANGLE MODEL_DEPTH MODEL_STEP MODEL_WSIZE MODEL_FEATURE_TYPE MODEL_FEATURE_HAAR_N MODEL_FEATURE_GAUSSIAN_N MODEL_FEATURE_GAUSSIAN_STD",
    "inputs": [
        {
        	"id": "cytomine_host",
      		"value-key": "@ID",
      		"command-line-flag": "--@id",
      		"name": "Cytomine host",
      		"set-by-server": true,
      		"optional": false,
      		"type": "String"
    	},
    	{
			"id": "cytomine_public_key",
			"value-key": "@ID",
			"command-line-flag": "--@id",
			"name": "Cytomine public key",
            "set-by-server": true,
			"optional": false,
			"type": "String"
    	},
    	{
    		"id": "cytomine_private_key",
			"value-key": "@ID",
			"command-line-flag": "--@id",
			"name": "Cytomine private key",
			"set-by-server": true,
			"optional": false,
			"type": "String"
    	},
		{
			"id": "cytomine_id_project",
			"value-key": "@ID",
			"command-line-flag": "--@id",
			"name": "Cytomine project ID",
			"set-by-server": true,
			"optional": false,
			"type": "Number"
    	},
		{
			"id": "cytomine_id_software",
			"value-key": "@ID",
			"command-line-flag": "--@id",
			"name": "Cytomine software ID",
			"set-by-server": true,
			"optional": false,
			"type": "Number"
		},
        {
            "id": "cytomine_training_images",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Cytomine image IDs",
            "set-by-server": false,
            "optional": false,
            "type": "ListDomain",
	        "uri": "/api/project/$currentProject$/imageinstance.json",
	        "uri-print-attribute": "instanceFilename",
	        "uri-sort-attribute": "instanceFilename"
        },
        {
            "id": "model_njobs",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Number of jobs",
            "set-by-server": false,
            "optional": false,
            "default-value":1,
            "type": "Number"
        },
        {
            "id": "model_RMAX",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Radius RMAX",
            "set-by-server": false,
            "optional": false,
            "default-value":100,
            "type": "Number"
        },
        {
            "id": "model_R",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Radius R",
            "set-by-server": false,
            "optional": false,
            "default-value":6,
            "type": "Number"
        },
        {
            "id": "model_P",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Not landmark proportion P",
            "set-by-server": false,
            "optional": false,
            "default-value":3,
            "type": "Number"
        },
        {
            "id": "model_npred",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Number of extracted pixels Np",
            "set-by-server": false,
            "optional": false,
            "default-value":50000,
            "type": "Number"
        },
        {
            "id": "model_ntrees",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Number of trees T",
            "set-by-server": false,
            "optional": false,
            "default-value":50,
            "type": "Number"
        },
        {
            "id": "model_ntimes",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Number of rotated versions per image Nt",
            "set-by-server": false,
            "optional": false,
            "default-value":1,
            "type": "Number"
        },
        {
            "id": "model_angle",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Max angle for rotated versions",
            "set-by-server": false,
            "optional": false,
            "default-value":10,
            "type": "Number"
        },
        {
            "id": "model_depth",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Multi-resolutions D",
            "set-by-server": false,
            "optional": false,
            "default-value":5,
            "type": "Number"
        },
        {
            "id": "model_step",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Stride s",
            "set-by-server": false,
            "optional": false,
            "default-value":1,
            "type": "Number"
        },
        {
            "id": "model_wsize",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Window size W",
            "set-by-server": false,
            "optional": false,
            "default-value":8,
            "type": "Number"
        },
        {
            "id": "model_feature_type",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Type of pixel descriptor",
            "set-by-server": false,
            "optional": false,
            "default-value":"raw",
            "type": "String"
        },
        {
            "id": "model_feature_haar_n",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Number of Haar-Features",
            "set-by-server": false,
            "optional": false,
            "default-value":1600,
            "type": "Number"
        },
        {
            "id": "model_feature_gaussian_n",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Number of Gaussian Features",
            "set-by-server": false,
            "optional": false,
            "default-value":1600,
            "type": "Number"
        },
        {
            "id": "model_feature_gaussian_std",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "STD (gaussian features)",
            "set-by-server": false,
            "optional": false,
            "default-value":20,
            "type": "Number"
        }]
}